<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Enrollment.
    -->
    <changeSet id="20230527162654-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="enrollment"
                                 constraintName="fk_enrollment__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="adademy_id"
                                 baseTableName="enrollment"
                                 constraintName="fk_enrollment__adademy_id"
                                 referencedColumnNames="id"
                                 referencedTableName="academy"/>

        <addForeignKeyConstraint baseColumnNames="course_id"
                                 baseTableName="enrollment"
                                 constraintName="fk_enrollment__course_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>
    </changeSet>
</databaseChangeLog>
